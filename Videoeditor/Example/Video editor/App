import SwiftUI

@main

struct YiVideoEditorApp: App {

    var body: some Scene {

        WindowGroup {

            NavigationView {

                HomeView()

            }

        }

    }

}

struct HomeView: View {

    var body: some View {

        Text("Welcome to YiVideoEditor!")

            .navigationBarTitle("Home", displayMode: .inline)

    }

}

